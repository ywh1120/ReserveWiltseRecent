            function reserve(time, p_num, p_name, desc, part, mrinum){
              if("#{user_id}" == ""){
                  $('#err_msg').empty();
                  $('#err_msg').append('아이디를 확인할 수 없습니다. 로그인이 필요합니다.');
                  $('#alert_box').modal('show');
                  return false;
              }
              if(p_num == ""){
                 $('#err_msg').empty();
                 $('#err_msg').append('환자번호를 입력해주세요.');
                 $('#alert_box').modal('show');
                 return false;
              }
              if(p_name == ""){
                 $('#err_msg').empty();
                 $('#err_msg').append('환자성명을 입력해주세요');
                 $('#alert_box').modal('show');
                 return false;
              }
              if(desc == ""){
                 $('#err_msg').empty();
                 $('#err_msg').append('검사명을 선택하십시오.');
                 $('#alert_box').modal('show');
                 return false;
              }    
              if(part == ""){
                 $('#err_msg').empty();
                 $('#err_msg').append('부서명을 선택하십시오.');
                 $('#alert_box').modal('show');
                 return false;
              }
              $.ajax({
                url:"/reservate",
                type:"POST",
                data:"resdate="+$('#datetimepicker12').data('date')+"&time="+time+"&patient_number="+p_num+"&patient_name="+p_name+"&description="+desc+"&part="+part+"&mrinum="+mrinum,
                success: function(data){
                  if(data == 'success'){
                    
                    socket.emit('toclient',{msg:'ins'});
                    //window.location.reload(true);
            	  }
                  else if(data == 'timeout'){
            	    $('#err_msg').empty();
               	    $('#err_msg').append('예약가능 시간이 아닙니다. 예약은 20분전까지 가능합니다.');
              	    $('#alert_box').modal('show');
                  }
                  else if(data == 'already'){
                    $('#err_msg').empty();
               	    $('#err_msg').append('이미 예약된 시간입니다.');
              	    $('#alert_box').modal('show');
              	    //window.location.reload(true);
                  }
                  else if(data == 'dupl'){
                    $('#err_msg').empty();
               	    $('#err_msg').append('foot-ankle, elbow는 동일 시간대 예약하실 수 없습니다.');
              	    $('#alert_box').modal('show');
              	    //window.location.reload(true);
                  }
                 },
                error: function(err){
                  alert('데이터 작성 중 오류가 발생하였습니다.');
                  alert(err);
                  }    
              });
